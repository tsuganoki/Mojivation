[Unit]
Description=Mojivation App
After=network.target

[Service]
User=ubuntu
Group=ubuntu
Environment="LANG=en_US.UTF-8"
Environment="LANGUAGE=en_US.UTF-8:"
WorkingDirectory=/home/ubuntu/Mojivation/
ExecStart=/bin/bash -c "source env/bin/activate\
; python3 -m http.server&>> flaskcss.log"
Restart=always

[Install]
WantedBy=multi-user.target